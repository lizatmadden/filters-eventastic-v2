{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"version": "1.1.0",
	"name": "madden-theme/search-results-filter",
	"title": "[MM] Search Results & Filters",
	"description": "Search engine for events, listings, or posts",
	"icon": "embed-photo",
	"category": "madden-media",
	"render": "file:./render.php",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"viewScript": "file:./view.js",
	"viewStyle": "file:./style-view.css",
	"supports": {
		"anchor": true
	},
	"attributes": {
		"postType": {
			"type": "string",
			"default": "post"
		},
		"listingType": {
			"type": "string",
			"default": ""
		},
		"subType": {
			"type": "string",
			"default": ""
		},
		"childTerms": {
			"type": "array",
			"default": []
		},
		"displayFilters": {
			"type": "boolean",
			"default": true
		},
		"citiesFilter": {
			"type": "boolean",
			"default": false
		},
		"selectedcities": {
			"type": "string",
			"default": ""
		},
		"categoryFilter": {
			"type": "boolean",
			"default": false
		},
		"selectedCategory": {
			"type": "string",
			"default": ""
		},
		"datesFilter": {
			"type": "boolean",
			"default": false
		},
		"selectedDates": {
			"type": "string",
			"default": ""
		},
		"perPage": {
			"type": "number",
			"default": 12
		},
		"title": {
			"type": "string",
			"default": ""
		},
		"description": {
			"type": "string",
			"default": ""
		}
	}
}